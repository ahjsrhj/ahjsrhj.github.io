doctype
html(lang=config.language)
    head
        include head
    body
        .wrap
            header
                a.logo-link(href=url_for())
                    img(src=url_for(theme.logo) alt="logo")
                include nav
                script(src='//cdn.bootcss.com/canvas-nest.js/1.0.0/canvas-nest.min.js')
            main.container
                block container
            footer
                block pagination
                block copyright
        include scripts
